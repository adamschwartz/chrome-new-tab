{
  "author": "Adam Schwartz",
  "name": "New Tab",
  "version": "13.0",
  "manifest_version": 2,
  "minimum_chrome_version": "103",
  "description": "A minimalist replacement for Chrome's New Tab page.",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "32": "icons/icon32.png",
    "16": "icons/icon16.png"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "web_accessible_resources": [
    "newtab.html"
  ]
}
