{
  "author": "Adam Schwartz",
  "name": "New Tab",
  "version": "100.0",
  "manifest_version": 3,
  "minimum_chrome_version": "37",
  "description": "A minimalist replacement for Chrome's New Tab page.",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "permissions": [
    "favicon",
    "bookmarks",
    "topSites",
    "management",
    "tabs",
    "sessions"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
